{
  "files": [],
  "references": [
    { "path": "packages/state-packs" },
    { "path": "packages/domain" }
  ],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "newLine": "lf",
